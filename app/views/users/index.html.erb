<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>
<h1>Listing Users</h1>

<table>
	<tr>
		<th>Email</th>
		<th>Name</th>
	</tr>

	<% @users.each do |user| %>
		<tr>
			<td><%= user.email %></td>
			<td><%= user.name %></td>
			<td><%= link_to 'Delete', user_path(user),
              method: :delete,
              data: { confirm: 'Are you sure?' } %></td>
		</tr>
	<% end %>
</table>
</table>